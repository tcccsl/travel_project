{
	"pages": [
		{
			"path": "pages/Home/Home",
			"style": {
				"navigationBarTitleText": "旅行日记",
				"enablePullDownRefresh": true,
				"app-plus": {
					"animationType": "fade-in-out",
					"animationDuration": 300
				}
			},
			"alias": "/"
		},
		{
			"path": "pages/DiaryDetail/DiaryDetail",
			"style": {
				"navigationBarTitleText": "游记详情",
				"navigationStyle": "custom",
				"app-plus": {
					"animationType": "fade-in-out",
					"animationDuration": 300
				}
			},
			"alias": "/diary-detail"
		},
		{
			"path": "pages/MyDiaries/MyDiaries",
			"style": {
				"navigationBarTitleText": "我的游记",
				"enablePullDownRefresh": true,
				"app-plus": {
					"animationType": "fade-in-out",
					"animationDuration": 300
				}
			},
			"alias": "/my-diaries"
		},
		{
			"path": "pages/Publish/Publish",
			"style": {
				"navigationBarTitleText": "发布游记",
				"app-plus": {
					"animationType": "fade-in-out",
					"animationDuration": 300
				}
			},
			"alias": "/publish"
		},
		{
			"path": "pages/Login/Login",
			"style": {
				"navigationBarTitleText": "登录",
				"navigationStyle": "custom",
				"app-plus": {
					"animationType": "fade-in-out",
					"animationDuration": 300
				}
			},
			"alias": "/login"
		},
		{
			"path": "pages/Register/Register",
			"style": {
				"navigationBarTitleText": "注册",
				"navigationStyle": "custom",
				"app-plus": {
					"animationType": "fade-in-out",
					"animationDuration": 300
				}
			},
			"alias": "/register"
		},
		{
			"path": "pages/Admin/Admin",
			"style": {
				"navigationBarTitleText": "游记管理系统",
				"navigationStyle": "custom",
				"app-plus": {
					"animationType": "fade-in-out",
					"animationDuration": 300
				}
			},
			"alias": "/admin"
		},
		{
			"path": "pages/AdminLogin/AdminLogin",
			"style": {
				"navigationBarTitleText": "管理员登录",
				"navigationStyle": "custom",
				"app-plus": {
					"animationType": "fade-in-out",
					"animationDuration": 300
				}
			},
			"alias": "/admin-login"
		}
	],
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "旅行日记",
		"navigationBarBackgroundColor": "#3B82F6",
		"backgroundColor": "#F8F8F8",
		"app-plus": {
			"animationType": "fade-in-out",
			"animationDuration": 300
		}
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#3B82F6",
		"backgroundColor": "#FFFFFF",
		"list": [
			{
				"pagePath": "pages/Home/Home",
				"text": "首页",
				"iconPath": "static/icons/home.png",
				"selectedIconPath": "static/icons/home-active.png"
			},
			{
				"pagePath": "pages/MyDiaries/MyDiaries",
				"text": "我的游记",
				"iconPath": "static/icons/diary.png",
				"selectedIconPath": "static/icons/diary-active.png"
			}
		]
	},
	"easycom": {
		"autoscan": true,
		"custom": {
			"^uni-(.*)": "@dcloudio/uni-ui/lib/uni-$1/uni-$1.vue"
		}
	},
	"h5": {
		"router": {
			"mode": "history",
			"base": "/"
		},
		"title": "旅行日记",
		"domain": "https://travel-diary.example.com"
	}
}
